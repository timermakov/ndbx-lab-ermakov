{
	"info": {
		"name": "EventHub",
		"description": "API collection for EventHub service",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"variable": [
		{
			"key": "base_url",
			"value": "http://localhost:8080"
		}
	],
	"item": [
		{
			"name": "Health",
			"item": [
				{
					"name": "Healthcheck",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/health",
							"host": ["{{base_url}}"],
							"path": ["health"]
						},
						"description": "Check service availability. Returns {\"status\":\"ok\"} with 200 OK."
					},
					"response": [
						{
							"name": "200 OK",
							"status": "OK",
							"code": 200,
							"header": [
								{
									"key": "Content-Type",
									"value": "application/json"
								}
							],
							"body": "{\"status\":\"ok\"}"
						}
					]
				}
			]
		}
	]
}
